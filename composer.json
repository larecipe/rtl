{
    "name": "binarytorch/larecipe-rtl",
    "description": "Official LaRecipe RTL Support.",
    "keywords": [
        "laravel",
        "larecipe"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Binarytorch\\LarecipeRtl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Binarytorch\\LarecipeRtl\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
